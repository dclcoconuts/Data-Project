DROP TABLE IF EXISTS Departements ;
CREATE TABLE Departements (ID_Departements INT(10) AUTO_INCREMENT NOT NULL,
REGION_CODE_Departements INT(10),
CODE_Departements INT(10),
NAME_Departements VARCHAR(100),
SLUG_Departements VARCHAR(100),
ID_Regions **NOT FOUND**(10),
prefecture_id_prefecture **NOT FOUND**(10),
PRIMARY KEY (ID_Departements)) ENGINE=InnoDB;

DROP TABLE IF EXISTS Regions ;
CREATE TABLE Regions (ID_Regions INT(5) AUTO_INCREMENT NOT NULL,
CODE_Regions INT(10),
NAME_Regions VARCHAR(100),
SLUG_Regions VARCHAR(100),
PRIMARY KEY (ID_Regions)) ENGINE=InnoDB;

DROP TABLE IF EXISTS Photos ;
CREATE TABLE Photos (ID_Photos INT(10) AUTO_INCREMENT NOT NULL,
DPT_Photos INT(2),
COMMUNE_Photos VARCHAR(255),
EDIFICE_Photos VARCHAR(255),
LEGENDE_Photos VARCHAR(255),
AUTEUR_Photos VARCHAR(255),
DATE_Photos DATETIME,
MINIATURE_Photos VARCHAR(255),
IMAGE_Photos VARCHAR(255),
ID_Departements **NOT FOUND**(10),
PRIMARY KEY (ID_Photos)) ENGINE=InnoDB;

DROP TABLE IF EXISTS Ville ;
CREATE TABLE Ville (ID_Ville INT(2) AUTO_INCREMENT NOT NULL,
DEPARTEMENT_Ville INT(2),
REGION_CODE_Ville INT(10),
NOM_Ville VARCHAR(255),
SLUG_Ville VARCHAR(255),
GPS_LAT_Ville INT(50),
GPS_ING_Ville VARCHAR(50),
ID_Departements **NOT FOUND**(2),
PRIMARY KEY (ID_Ville)) ENGINE=InnoDB;

DROP TABLE IF EXISTS PREFECTURE ;
CREATE TABLE PREFECTURE (ID_PREFECTURE INT(2) AUTO_INCREMENT NOT NULL,
REGION_CODE_PREFECTURE INT(5),
DEPARTEMENT_PREFECTURE VARCHAR(100),
PREFECTURE_PREFECTURE VARCHAR(100),
REGION_PREFECTURE VARCHAR(100),
departements_id_departements **NOT FOUND**(2),
PRIMARY KEY (ID_PREFECTURE)) ENGINE=InnoDB;

ALTER TABLE Departements ADD CONSTRAINT FK_Departements_ID_Regions FOREIGN KEY (ID_Regions) REFERENCES Regions (ID_Regions);

ALTER TABLE Departements ADD CONSTRAINT FK_Departements_prefecture_id_prefecture FOREIGN KEY (prefecture_id_prefecture) REFERENCES PREFECTURE (ID_PREFECTURE);
ALTER TABLE Photos ADD CONSTRAINT FK_Photos_ID_Departements FOREIGN KEY (ID_Departements) REFERENCES Departements (ID_Departements);
ALTER TABLE Ville ADD CONSTRAINT FK_Ville_ID_Departements FOREIGN KEY (ID_Departements) REFERENCES Departements (ID_Departements);
ALTER TABLE PREFECTURE ADD CONSTRAINT FK_PREFECTURE_departements_id_departements FOREIGN KEY (departements_id_departements) REFERENCES Departements (ID_Departements);
